}
header a.fresh {
	background: url("fresh.png") no-repeat 50% 50% / 32px 32px;
}
header a.home {
	background: url("../images/home.png") no-repeat 50% 50% / 36px 36px;
}
header a.tel {
	background: url("call.png") no-repeat 50% 50% / 36px 36px;
}
section.topfixed {
	background: rgba(68, 54, 28, 0.9); margin: 0px auto; padding: 0px; width: 100%; height: auto; text-align: left; color: rgb(255, 255, 255); line-height: 25px; overflow: hidden; position: fixed; z-index: 999; box-shadow: 0px 3px 3px rgba(30,30,30,0.6); text-shadow: none;
}
section.topfixed p {
