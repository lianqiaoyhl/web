header {
	background: rgb(0, 0, 0); margin: 0px auto; padding: 0px; width: 100%; height: 40px; text-align: center; color: rgb(255, 255, 255); line-height: 40px; overflow: hidden; border-top-color: rgb(51, 51, 51); border-top-width: 1px; border-top-style: solid; white-space: nowrap; position: relative;
}
header img {
