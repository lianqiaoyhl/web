}
ul {
	list-style: none;
}
ol {
	list-style: none;
}
li {
	list-style: none;
}
.fl {
	float: left;
}
.fr {
	float: right;
}
.clearfix::after {
	height: 0px; overflow: hidden; clear: both; display: block; visibility: hidden; content: ".";
}
.clearfix {
	display: inline-block;
}
.clr {
	clear: both;
}
.hidden {
	display: none !important;
}
* {
