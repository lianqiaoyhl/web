}
nav ul.Transverse li a strong {
	line-height: 45px; padding-left: 32px; font-size: 14px; font-weight: normal; display: inline; position: inherit;
}
nav ul.Transverse li:nth-child(1) a strong {
	background: url("../images/home.png") no-repeat left / 28px 28px;
}
nav ul.Transverse li:nth-child(2) a strong {
	background: url("call.png") no-repeat left / 28px 28px;
}
nav ul.Transverse li:nth-child(3) a strong {
	background: url("sms.png") no-repeat left / 28px 28px;
}
nav ul.Transverse li:nth-child(1) a.home strong {
	background: url("../images/home.png") no-repeat left / 28px 28px;
}
nav ul.Transverse li span {
	color: rgb(51, 51, 51);
}
